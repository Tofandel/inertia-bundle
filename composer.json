{
  "name": "tofandel/inertia-bundle",
  "description": "A Symfony adapter for Inertia.js",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Hannes Vermeire",
      "email": "hannesvermeire@gmail.com"
    },
    {
      "name": "Adrien Foulon",
      "email": "tofandel@tukan.hu"
    }
  ],
  "autoload": {
    "psr-4": {
      "Rompetomp\\InertiaBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rompetomp\\InertiaBundle\\Tests\\": "Tests"
    }
  },
  "require": {
    "php": "^8",
    "ext-json": "*",
    "symfony/config": "^6.0|^7.0",
    "symfony/yaml": "^6.0|^7.0",
    "symfony/dependency-injection": "^6.0|^7.0",
    "symfony/http-kernel": "^6.0|^7.0",
    "symfony/http-client": "^6.1|^7.0",
    "twig/twig": "^2.12|^3.0",
    "illuminate/support": "^9|^10|^11|^12"
  },
  "suggest": {
    "symfony/form": "^7"
  },
  "require-dev": {
    "laravel/pint": "^1",
    "symfony/form": "^7",
    "symfony/validator": "^7",
    "symfony/webpack-encore-bundle": "^2",
    "symfony/framework-bundle": "^7.0",
    "symfony/twig-bundle": "^7.0",
    "mockery/mockery": "^1.5",
    "symfony/phpunit-bridge": "^7.0",
    "symfony/property-access": "^7.0",
    "symfony/serializer": "^7.0",
    "symfony/var-dumper": "^7.0",
    "phpunit/phpunit": "^12"
  }
}
